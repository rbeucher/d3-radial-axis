{
  "name": "d3-radial-axis",
  "version": "1.6.1",
  "description": "Radial axes for D3 components.",
  "keywords": [
    "d3",
    "d3-module",
    "axis",
    "radial",
    "polar",
    "spiral",
    "scale",
    "visualization"
  ],
  "unpkg": "dist/d3-radial-axis.min.js",
  "main": "dist/d3-radial-axis.js",
  "module": "src/index",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/d3-radial-axis.git"
  },
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "homepage": "https://github.com/vasturiano/d3-radial-axis#readme",
  "files": [
    "src/**/*",
    "dist/**/*",
    "example/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist && mkdir dist && rollup -c",
    "watch": "rollup -c -w",
    "minify": "uglifyjs dist/d3-radial-axis.js -o dist/d3-radial-axis.min.js -c -m --comments '/Version/'",
    "prepare": "npm run build && npm run minify"
  },
  "dependencies": {
    "d3-scale": "^2.2.2",
    "d3-shape": "^1.3.3"
  },
  "devDependencies": {
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-watch": "^4.3.1",
    "uglify-js": "^3.4.9"
  }
}
